<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>NoticiaSV</servlet-name>
        <servlet-class>com.portalgoiano.consultar.controller.NoticiaSV</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>recursoSV</servlet-name>
        <servlet-class>com.portalgoiano.consultar.controller.RecursoSV</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UnidadeSV</servlet-name>
        <servlet-class>com.portalgoiano.consultar.controller.UnidadeSV</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NoticiaSV</servlet-name>
        <url-pattern>/NoticiaSV</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>recursoSV</servlet-name>
        <url-pattern>/recursoSV</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UnidadeSV</servlet-name>
        <url-pattern>/UnidadeSV</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
